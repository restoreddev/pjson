    "type": "library",
    "require": {
        "php": ">=5.6",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Davis",
            "email": "andrew@restoredsoftware.com"
        }
    ],
    "minimum-stability": "beta",
    "autoload": {
        "psr-4": {
            "Pjson\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pjson\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
